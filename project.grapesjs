ode","content":"81 Books"}]},{"tagName":"span","classes":["bubble-text"],"attributes":{"aria-hidden":"true","id":"i5f7mi"}}]}]},{"type":"button","classes":["bubble-button"],"attributes":{"type":"button","data-fact":"I cannot tolerate the taste of olives!","aria-label":"Reveal fact: I cannot tolerate the taste of olives!","aria-pressed":"false","aria-expanded":"false","id":"ij8127"},"components":[{"type":"contentEditable","components":[{"tagName":"span","type":"text","classes":["bubble-label"],"attributes":{"id":"iygdmk"},"components":[{"type":"textnode","content":"No Olives"}]},{"tagName":"span","classes":["bubble-text"],"attributes":{"aria-hidden":"true","id":"i7ncxh"}}]}]}]},{"type":"script","attributes":{"id":"ie61pe"},"components":[{"type":"textnode","content":"\n                (function() {\n                  var root = document.getElementById('i6zxcf');\n                  if (!root) return;\n\n                  var bubbles = Array.from(root.querySelectorAll('.bubble-button'));\n                  var timers = new WeakMap();\n\n                  function getMinDims(btn) {\n                    var vw = window.innerWidth || 1200;\n                    var min = 84; // desktop default\n                    if (vw <= 480) min = 68;\n                    else if (vw <= 992) min = 76;\n\n                    // Use actual current size as baseline if available (captures any CSS tweaks)\n                    var r = btn.getBoundingClientRect();\n                    var ow = Math.round(r.width || min);\n                    var oh = Math.round(r.height || min);\n                    // Guarantee not smaller than intended min\n                    return {\n                      w: Math.max(min, ow),\n                      h: Math.max(min, oh)\n                    };\n                  }\n\n                  function storeOriginalSize(btn) {\n                    if (!btn.dataset.ow || !btn.dataset.oh) {\n                      var r = btn.getBoundingClientRect();\n                      btn.dataset.ow = String(Math.round(r.width));\n                      btn.dataset.oh = String(Math.round(r.height));\n                    }\n                  }\n\n                  function clearTimer(btn) {\n                    var t = timers.get(btn);\n                    if (t) {\n                      clearTimeout(t);\n                      timers.delete(btn);\n                    }\n                  }\n\n                  function hideBubble(btn) {\n                    if (!btn) return;\n\n                    clearTimer(btn);\n\n                    // Begin by hiding text (fade) before shrinking, to meet spec\n                    btn.classList.remove('show-text');\n                    btn.setAttribute('aria-pressed', 'false');\n                    btn.setAttribute('aria-expanded', 'false');\n\n                    var span = btn.querySelector('.bubble-text');\n\n                    // After text fade-out, animate back to original size\n                    var shrinkDelay = 200; // after text opacity transition\n                    var ow = parseInt(btn.dataset.ow || '0', 10) || getMinDims(btn).w;\n                    var oh = parseInt(btn.dataset.oh || '0', 10) || getMinDims(btn).h;\n\n                    setTimeout(function() {\n                      btn.style.width = ow + 'px';\n                      btn.style.height = oh + 'px';\n                    }, shrinkDelay);\n\n                    // After the size transition, cleanup styles and content\n                    var cleanupDelay = shrinkDelay + 340; // match transition (320ms) + small buffer\n                    setTimeout(function() {\n                      if (span) {\n                        span.textContent = '';\n                        span.style.maxWidth = '';\n                      }\n                      btn.classList.remove('expand-open', 'is-popping');\n                      btn.style.width = '';\n                      btn.style.height = '';\n                    }, cleanupDelay);\n                  }\n\n                  function hideAll(except) {\n                    bubbles.forEach(function(b) {\n                      if (b !== except) hideBubble(b);\n                    });\n                  }\n\n                  function popAndReveal(btn) {\n                    if (!btn) return;\n\n                    hideAll(btn);\n                    clearTimer(btn);\n                    storeOriginalSize(btn);\n\n                    // Prepare for expansion and pop\n                    btn.classList.remove('is-popping');\n                    void btn.offsetWidth; // reflow to restart animation\n                    btn.classList.add('expand-open');\n\n                    var fact = btn.getAttribute('data-fact') || '';\n                    var span = btn.querySelector('.bubble-text');\n                    if (!span) return;\n\n                    // Put content in span (still hidden by default)\n                    span.textContent = fact;\n\n                    // Measure desired target size\n                    // 1) Single-line width without max constraint\n                    var prevMax = span.style.maxWidth;\n                    span.style.maxWidth = 'none';\n                    var singleLineW = Math.ceil(span.scrollWidth);\n\n                    // Sane max width per viewport/container\n                    var containerW = root.getBoundingClientRect().width || 600;\n                    var maxW = Math.max(200, Math.min(360, Math.floor(containerW - 24)));\n\n                    // Padding applied when expanded\n                    var padX = 28; // 14px * 2\n                    var padY = 24; // 12px * 2\n\n                    var base = getMinDims(btn);\n                    var targetW = Math.max(base.w, Math.min(singleLineW + padX, maxW));\n\n                    // Apply width first so the text wraps accordingly, then measure height\n                    btn.style.width = targetW + 'px';\n\n                    // Constrain text to the inner content area to avoid edge clipping\n                    span.style.maxWidth = Math.max(60, targetW - padX) + 'px';\n\n                    // Force reflow then compute height at this width\n                    void btn.offsetWidth;\n                    var targetH = Math.max(base.h, Math.ceil(span.scrollHeight + padY));\n                    btn.style.height = targetH + 'px';\n\n                    // Synchronize size transition with the pop effect\n                    btn.classList.add('is-popping');\n                    btn.classList.add('show-text');\n                    btn.setAttribute('aria-pressed', 'true');\n                    btn.setAttribute('aria-expanded', 'true');\n\n                    // Auto-hide and return to original size\n                    var tid = setTimeout(function() {\n                      hideBubble(btn);\n                    }, 3500);\n                    timers.set(btn, tid);\n\n                    // Restore previous inline max after animation is done (keeps layout predictable)\n                    setTimeout(function() {\n                      if (btn.classList.contains('show-text')) return; // still open; keep max\n                      span.style.maxWidth = prevMax || '';\n                    }, 400);\n                  }\n\n                  // Attach click handlers to bubbles\n                  bubbles.forEach(function(btn) {\n                    btn.addEventListener('click', function() {\n                      popAndReveal(btn);\n                    });\n                  });\n\n                  // Optional \"New Fact\" trigger if present, will randomly expand one bubble\n                  var btnNew = root.querySelector('#btnNewFact');\n                  if (btnNew) {\n                    btnNew.addEventListener('click', function(e) {\n                      e.preventDefault();\n                      if (!bubbles.length) return;\n                      var pick = bubbles[Math.floor(Math.random() * bubbles.length)];\n                      popAndReveal(pick);\n                    });\n                  }\n                })();\n              "}]}]},{"tagName":"article","classes":["contact-card"],"attributes":{"id":"iko0ef"},"components":[{"tagName":"h3","type":"heading","classes":["contact-title"],"attributes":{"id":"ikotxr"},"components":[{"type":"textnode","content":"Say Hello!"}]},{"tagName":"p","type":"text","classes":["contact-copy"],"attributes":{"id":"ihxb5l"},"components":[{"type":"textnode","content":"Should work now!"}]},{"type":"form","classes":["contact-form"],"attributes":{"method":"POST","action":"https://formsubmit.co/thejomanade@gmail.com","autocomplete":"on","id":"ise45f"},"components":[{"type":"comment","content":" FormSubmit configuration "},{"type":"input","void":true,"attributes":{"type":"hidden","name":"_subject","value":"New message ‚Äî The Pearl Portfolio"}},{"type":"input","void":true,"attributes":{"type":"hidden","name":"_template","value":"table"}},{"type":"input","void":true,"attributes":{"type":"text","name":"_honey","tabindex":"-1","autocomplete":"off","id":"ijx625"}},{"type":"input","void":true,"attributes":{"type":"hidden","name":"_captcha","value":"false"}},{"classes":["form-row"],"attributes":{"id":"i2dsy6"},"components":[{"classes":["form-field"],"components":[{"type":"label","attributes":{"for":"cf-name"},"components":[{"type":"textnode","content":"Name"}]},{"type":"input","void":true,"attributes":{"type":"text","name":"name","placeholder":"Your name","required":true,"id":"cf-name"}}]},{"classes":["form-field"],"components":[{"type":"label","attributes":{"for":"cf-email"},"components":[{"type":"textnode","content":"Email"}]},{"type":"input","void":true,"attributes":{"type":"email","name":"email","placeholder":"you@example.com","required":true,"id":"cf-email"}}]}]},{"tagName":"fieldset","classes":["form-fieldset"],"attributes":{"aria-labelledby":"cf-gender-label"},"components":[{"tagName":"legend","type":"text","attributes":{"id":"cf-gender-label"},"components":[{"type":"textnode","content":"Gender"}]},{"classes":["choices"],"attributes":{"id":"iy7gjk"},"components":[{"type":"label","classes":["choice"],"attributes":{"for":"cf-gender-m"},"components":[{"type":"checkbox","void":true,"attributes":{"type":"checkbox","name":"gender[]","value":"M","id":"cf-gender-m"}},{"tagName":"span","type":"text","components":[{"type":"textnode","content":"M"}]}]},{"type":"label","classes":["choice"],"attributes":{"for":"cf-gender-f"},"components":[{"type":"checkbox","void":true,"attributes":{"type":"checkbox","name":"gender[]","value":"F","id":"cf-gender-f"}},{"tagName":"span","type":"text","components":[{"type":"textnode","content":"F"}]}]}]}]},{"classes":["form-field"],"components":[{"type":"label","attributes":{"for":"cf-message"},"components":[{"type":"textnode","content":"Message"}]},{"type":"textarea","attributes":{"name":"message","rows":"5","placeholder":"Write your message...","required":true,"id":"cf-message"}}]},{"classes":["actions"],"components":[{"type":"button","classes":["t-button","submit-btn"],"attributes":{"type":"submit"},"components":[{"type":"contentEditable","components":[{"type":"textnode","content":"Send Message ‚Üí"}]}]}]}]},{"classes":["contact-actions"],"attributes":{"id":"izjz7h"}}]}]}]},{"classes":["ripples-wave-separator"],"attributes":{"id":"iz07wl"}}]}]},{"tagName":"footer","classes":["footer-section"],"attributes":{"id":"iig6c2"},"components":[{"classes":["footer-container"],"attributes":{"id":"igfwpd"},"components":[{"tagName":"p","type":"text","classes":["footer-text"],"attributes":{"id":"i5b6xj"},"components":[{"type":"textnode","content":"\n          Just like pearls, people are shaped by time and experience. Thanks for visiting my little ocean.\n        "}]},{"tagName":"small","classes":["footer-copyright"],"attributes":{"id":"i2atnl"},"components":[{"type":"textnode","content":"¬© "},{"tagName":"span","attributes":{"id":"yearNow"}},{"type":"textnode","content":" Jomana Hamdy"}]}]}]},{"type":"script","attributes":{"id":"i4xl9k"},"components":[{"type":"textnode","content":"\n      // Year\n      document.getElementById('yearNow').textContent = new Date().getFullYear();\n\n      // Smooth dive in\n      const btnDive = document.getElementById('btnDive');\n      if (btnDive) {\n        btnDive.addEventListener('click', () => {\n          const el = document.querySelector('#about');\n          if (el) {\n            el.scrollIntoView({\n              behavior: 'smooth',\n              block: 'start'\n            });\n          }\n        });\n      }\n\n      // Ripple effect for buttons (extended to all actionable buttons)\n      function attachRipple(el) {\n        el.addEventListener('click', function(e) {\n          const rect = this.getBoundingClientRect();\n          const ripple = document.createElement('span');\n          ripple.className = 'ripple';\n          ripple.style.left = (e.clientX - rect.left) + 'px';\n          ripple.style.top = (e.clientY - rect.top) + 'px';\n          this.appendChild(ripple);\n          ripple.addEventListener('animationend', () => ripple.remove());\n        });\n      }\n      document.querySelectorAll('.btn-ripple, .t-button, .bubble-button, .easter-egg-pearl-button').forEach(attachRipple);\n\n      // Cursor bubble trail\n      document.addEventListener('mousemove', (e) => {\n        const b = document.createElement('span');\n        b.className = 'cursor-bubble';\n        const size = 6 + Math.random() * 8;\n        b.style.width = size + 'px';\n        b.style.height = size + 'px';\n        b.style.left = e.clientX + 'px';\n        b.style.top = e.clientY + 'px';\n        document.body.appendChild(b);\n        setTimeout(() => b.remove(), 1200);\n      });\n\n      // Stars reveal on intersect (fixed selector + twinkle)\n      const horizon = document.querySelector('#horizon');\n      if ('IntersectionObserver' in window && horizon) {\n        const stars = horizon.querySelectorAll('.horizon-star');\n        const ioStars = new IntersectionObserver((entries) => {\n          entries.forEach(en => {\n            if (en.isIntersecting) {\n              stars.forEach((s, i) => {\n                setTimeout(() => {\n                  s.style.opacity = '1';\n                  s.classList.add('twinkle');\n                }, 100 * i);\n              });\n              ioStars.unobserve(en.target);\n            }\n          });\n        }, {\n          threshold: 0.2\n        });\n        ioStars.observe(horizon);\n      }\n\n      // Random fact generator\n      const factOutput = document.getElementById('factOutput');\n      const bubbles = document.querySelectorAll('[data-fact]');\n      const btnNew = document.getElementById('btnNewFact');\n      const factPool = Array.from(bubbles).map(b => b.getAttribute('data-fact'));\n\n      function showRandomFact() {\n        if (!factPool.length) return;\n        const pick = factPool[Math.floor(Math.random() * factPool.length)];\n        factOutput.textContent = pick;\n      }\n      bubbles.forEach(b => b.addEventListener('click', () => {\n        factOutput.textContent = b.getAttribute('data-fact');\n      }));\n      if (btnNew) btnNew.addEventListener('click', showRandomFact);\n\n      // Easter egg pearl\n      const pearlBtn = document.getElementById('tinyPearl');\n      const pearlQuote = document.getElementById('pearlQuote');\n      if (pearlBtn && pearlQuote) {\n        pearlBtn.addEventListener('click', () => {\n          pearlQuote.classList.toggle('hidden');\n          // Fallback to inline toggle if no .hidden style is defined\n          if (getComputedStyle(pearlQuote).display === 'none') {\n            pearlQuote.style.display = 'block';\n          } else if (!pearlQuote.classList.contains('hidden')) {\n            pearlQuote.style.display = '';\n          }\n        });\n      }\n\n      // Animate On Scroll (AOS) system - subtle fade/slide and zoom reveals\n      (function() {\n        if (!('IntersectionObserver' in window)) return;\n\n        const prefersReduced = window.matchMedia && window.matchMedia('(prefers-reduced-motion: reduce)').matches;\n\n        const slideSelectors = [\n          '.hero-container',\n          '.hero-heading',\n          '.hero-subheading',\n          '.about-container',\n          '.about-grid',\n          '.about-heading',\n          '.about-intro-copy',\n          '.about-bullets-group',\n          '.treasures-container',\n          '.treasures-header-row',\n          '.treasures-grid',\n          '.horizon-container',\n          '.horizon-grid',\n          '.horizon-text-column',\n          '.horizon-heading',\n          '.horizon-intro',\n          '.horizon-goals-list',\n          '.ripples-container',\n          '.ripples-header-row',\n          '.ripples-grid'\n        ];\n\n        const zoomSelectors = [\n          '#iuqbwm',\n          '.about-profile-image',\n          '.treasures-chest-image',\n          '.moodboard-item img',\n          '.treasure-card',\n          '.random-fact-card',\n          '.easter-egg-card',\n          '.mini-blog-card',\n          '.mini-blog-item',\n          '.contact-card'\n        ];\n\n        const slideTargets = slideSelectors.flatMap(sel => Array.from(document.querySelectorAll(sel)));\n        const zoomTargets = zoomSelectors.flatMap(sel => Array.from(document.querySelectorAll(sel)));\n\n        slideTargets.forEach(el => el.classList.add('aos-init'));\n        zoomTargets.forEach(el => el.classList.add('aos-zoom-init'));\n\n        if (prefersReduced) {\n          slideTargets.forEach(el => el.classList.add('aos-in'));\n          zoomTargets.forEach(el => el.classList.add('aos-zoom-in'));\n          return;\n        }\n\n        const io = new IntersectionObserver((entries) => {\n          entries.forEach(en => {\n            if (en.isIntersecting) {\n              const t = en.target;\n              if (t.classList.contains('aos-zoom-init')) {\n                t.classList.add('aos-zoom-in');\n                t.classList.remove('aos-zoom-init');\n              } else if (t.classList.contains('aos-init')) {\n                t.classList.add('aos-in');\n                t.classList.remove('aos-init');\n              }\n              io.unobserve(t);\n            }\n          });\n        }, {\n          threshold: 0.14,\n          rootMargin: '0px 0px -40px 0px'\n        });\n\n        slideTargets.concat(zoomTargets).forEach(el => io.observe(el));\n      })();\n    "}]}],"doctype":"<!DOCTYPE html>","head":{"type":"head","name":"Head Container","attributes":{"id":"iaip"},"components":[{"tagName":"title","type":"text","attributes":{"id":"iv6v"},"components":[{"type":"textnode","content":"Jomana Hamdy ‚Äî The Pearl Portfolio"}]},{"tagName":"meta","void":true,"attributes":{"charset":"UTF-8","id":"ivwy"}},{"tagName":"meta","void":true,"attributes":{"name":"viewport","content":"width=device-width, initial-scale=1.0","id":"imyg"}},{"tagName":"meta","void":true,"attributes":{"name":"robots","content":"index,follow"}},{"tagName":"meta","void":true,"attributes":{"name":"generator","content":"GrapesJS Studio"}},{"tagName":"link","void":true,"attributes":{"href":"https://fonts.googleapis.com/css2?family=Playfair+Display:wght@600;700&display=swap","rel":"stylesheet","id":"iw8n4"}},{"tagName":"link","void":true,"attributes":{"href":"https://fonts.googleapis.com/css2?family=Poppins:wght@300;400;500;600&display=swap","rel":"stylesheet","id":"iu3jf"}}]},"docEl":{"tagName":"html","attributes":{"lang":"en"}}},"id":"AAbfVY3RkTXKAK0N","minHeight":"500px"}],"id":"ULS0QuaZDd8JMvRS"},{"name":"Blog","frames":[{"component":{"type":"wrapper","attributes":{"id":"i7scqh"},"components":[{"tagName":"section","attributes":{"id":"main-blog-container"},"components":[{"classes":["container"],"attributes":{"id":"iy51pg"},"components":[{"classes":["header"],"attributes":{"id":"iq8oxg"},"components":[{"classes":["title"],"attributes":{"id":"i0qbv7"},"components":[{"tagName":"h2","type":"heading","classes":["t-h2"]